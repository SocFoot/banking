<div ng-controller"userController">
	<div ng-controller="viewController">
		<div ng-controller="dataController">
			<form form-resource="account" post="postaccount(account)" ng-data="data" ng-views="view" class="container" id="new_account" accept-charset="UTF-8">
				<div class="form-group row">
				    <label class="col-sm-2 form-control-label" for="account_zip">Zip:</label>
				    <div class="col-sm-10">
				    <input class="form-control" type="number" name="account[zip]" id="account_zip" />
					</div>
				</div>
				<div class="form-group row">
				    <label class="col-sm-2 form-control-label" for="account_libelle">Libelle:</label>
				    <div class="col-sm-10">
					    <input class="form-control" type="text" name="account[libelle]" 
					    	ng-model="data.account.libelle" id="account_libelle" />
				    </div>
				</div>
				<div class="form-group row">
				    <label class="col-sm-2 form-control-label" for="account_conseille_id">Conseille:</label>
					<div class="col-sm-10">
						<select select-resource="conseille"
							ng-model="data.account.conseille_id" 
							ng-views="view"
							ng-click="getdata('conseilles', 'data.conseilles',view.select.data_id_code)"
							class="btn form-control">			    		
					   	</select>
					</div>
		   		</div>
				<div class="actions">
					<input type="submit" name="commit" value="new" class="btn btn-secondary" 
				  		ng-click="postaccount({account:data.account})"/>
				</div>
			</form>
		</div>
	</div>
</div>